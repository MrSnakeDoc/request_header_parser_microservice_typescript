{
  "name": "request_header_parser_microservice_typescript",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "tsc",
    "prebuild": "rimraf build && tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node -r dotenv/config build/index.js",
    "dev": "NODE_ENV=dev nodemon --config nodemon.json",
    "build:watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-fix": "tslint --fix -c tslint.json -p tsconfig.json ",
    "test": "yarn build"
  },
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.29"
  }
}
